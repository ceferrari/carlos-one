{
  "name": "carlos-one",
  "description": "Carlos One",
  "author": {
    "name": "Carlos Eduardo Ferrari",
    "email": "email@email.com",
    "url": "https://github.com/ceferrari"
  },
  "repository": {
    "url": "https://github.com/ceferrari/carlos-one"
  },
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development ASPNETCORE_ENVIRONMENT=Development webpack --progress",
    "build:prod": "cross-env NODE_ENV=production ASPNETCORE_ENVIRONMENT=Production webpack --progress",
    "dev": "cross-env NODE_ENV=development ASPNETCORE_ENVIRONMENT=Development dotnet run -c Debug",
    "prod": "cross-env NODE_ENV=production ASPNETCORE_ENVIRONMENT=Production dotnet run -c Release",
    "clean:win": "dotnet clean && (if exist bin rd /s /q bin) & (if exist obj rd /s /q obj) & (if exist publish rd /s /q publish) & (if exist wwwroot rd /s /q wwwroot)",
    "clean:unix": "dotnet clean && rm -rf bin/ obj/ publish/ wwwroot/",
    "publish:win": "npm run clean:win && dotnet publish -c Release -o publish -r win-x64 --self-contained",
    "publish:unix": "npm run clean:unix && dotnet publish -c Release -o publish -r win-x64 --self-contained",
    "lint:config": "eslint --print-config . | eslint-config-prettier-check && stylelint-config-prettier-check",
    "lint:check": "eslint -c ./.eslintrc.js ./*.js ./*.json ClientApp/**/*.js ClientApp/**/*.vue && stylelint ClientApp/**/*.css",
    "lint:fix": "eslint -c ./.eslintrc.js --fix ./*.js ./*.json ClientApp/**/*.js ClientApp/**/*.vue && stylelint --fix ClientApp/**/*.css",
    "browsers-list": "npx browserslist",
    "update:check": "npx npm-check -u",
    "update:fix": "npx npm-check -y && npm audit fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@babel/runtime-corejs3": "^7.5.5",
    "axios": "^0.19.0",
    "core-js": "^3.1.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "overlayscrollbars": "^1.9.0",
    "regenerator-runtime": "^0.13.3",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "aspnet-webpack": "^3.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.1.0",
    "jquery": "^3.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.15.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 11",
    "maintained node versions",
    "not dead"
  ]
}
